<wl:adapter xmlns:wl="http://www.worklight.com/integration" xmlns:http="http://www.worklight.com/integration/http" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="TextToSpeech">

	<displayName>TextToSpeech</displayName>
	<description>TextToSpeech</description>
	<connectivity>
		<connectionPolicy xsi:type="http:HTTPConnectionPolicyType">
			<protocol>https</protocol>
			   <domain>api.att.com</domain> 
			<port>443</port>			
		</connectionPolicy>
		<loadConstraints maxConcurrentConnectionsPerNode="2"/>
	</connectivity>

	<procedure name="textToSpeech"/>
	
</wl:adapter>
